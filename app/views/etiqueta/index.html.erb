<p id="notice"><%= notice %></p>

<h1>Etiqueta</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @etiqueta.each do |etiquetum| %>
      <tr>
        <td><%= etiquetum.nombre %></td>
        <td><%= link_to 'Show', etiquetum %></td>
        <td><%= link_to 'Edit', edit_etiquetum_path(etiquetum) %></td>
        <td><%= link_to 'Destroy', etiquetum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Etiquetum', new_etiquetum_path %>
