<p id="notice"><%= notice %></p>

<h1>Answer Reports</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Answer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answer_reports.each do |answer_report| %>
      <tr>
        <td><%= answer_report.titulo %></td>
        <td><%= answer_report.answer_id %></td>
        <td><%= link_to 'Show', answer_report %></td>
        <td><%= link_to 'Edit', edit_answer_report_path(answer_report) %></td>
        <td><%= link_to 'Destroy', answer_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer Report', new_answer_report_path %>
